{
  "name": "angular2-tg-app",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "dev": "webpack --config webpack/webpack.dev.js --env.dev --progress --hide-modules --watch",
    "predev": "npm run clean",
    "prod": "webpack --config webpack/webpack.prod.js --env.prod --progress --hide-modules",
    "preprod": "npm run clean",
    "start": "webpack --config webpack/webpack.server.js --env.server --progress --hide-modules && webpack --config webpack/webpack.prod.js --env.prod --progress --hide-modules",
    "universal:watch": "webpack --config webpack/webpack.server.js --env.server --progress --hide-modules --watch",
    "prerender": "npm run clean",
    "universal": "webpack --config webpack/webpack.server.js --env.server --progress --hide-modules --watch",
    "server": "test -f ../web/dist/server.js && nodemon ../web/dist/server.js -L || echo Project is not rendered! Please run ``npm start`` before run server",
    "clean": "rimraf ../web/dist"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.0",
