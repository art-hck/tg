    "node-sass": "^4.12.0",
    "object-hash": "^1.3.0",
    "pug": "^2.0.3",
    "reset-css": "^2.2.1",
    "rxjs": "^6.3.3",
    "throttle-debounce": "^2.0.1",
    "universal-cookie": "^2.1.2",
    "webpack-manifest-plugin": "^2.0.4",
    "zone.js": "~0.8.26"
